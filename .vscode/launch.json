{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/build/test1.elf",
            "name": "Debug Microcontroller",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/stm32h7x.cfg"
            ],
            "overrideLaunchCommands": [
                "delete mem 2",
                "mem 0x90000000 0x90400000 ro",
                "mon reset halt",
                "b Reset_Handler",
                "continue"
            ]
        }
    ]
}